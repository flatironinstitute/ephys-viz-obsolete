{
  "name": "ephys-viz",
  "version": "0.6.13",
  "description": "",
  "scripts": {
    "test": "bin/ev-dataset kbucket://b5ecdf1474c5/datasets/synth_datasets/datasets/synth_tetrode_30min --timeout=4000",
    "start": "node ephysviz-webserver.js",
    "postinstall": "npm run webpack && npm run update_jp_ephys_viz",
    "webpack": "node --use-strict node_modules/webpack-cli/bin/webpack.js --devtool source-map --mode development",
    "update_jp_ephys_viz": "jp_ephys_viz/devel/update.sh",
    "deploy_jp_ephys_viz": "npm install && jp_ephys_viz/devel/update.sh && jp_ephys_viz/devel/publish.sh",
    "pack": "build --dir",
    "dist": "build"
  },
  "author": "Jeremy Magland",
  "license": "Apache v2",
  "dependencies": {
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.1.1",
    "d3": "^5.2.0",
    "electron": "^2.0.5",
    "express": "^4.16.3",
    "jquery": "^3.3.1",
    "kbclient": "latest",
    "octicons": "4.3.0",
    "popper.js": "^1.14.3",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3"
  },
  "directories": {
    "bin": "bin"
  },
  "repository": {
    "url": "https://github.com/flatironinstitute/ephys-viz"
  },
  "conda": {
    "build_number": 0,
    "build_script": [
      "tgzfile=$(npm pack)",
      "npm install -g $tgzfile",
      "python -m pip install vdom jp_proxy_widget",
      "python -m pip install --no-deps --ignore-installed jp_ephys_viz"
    ],
    "requirements": [
      "python",
      "pip",
      "nodejs",
      "ipython",
      "ipywidgets"
    ],
    "test_commands": [
      "which ev-timeseries"
    ],
    "test_imports": [
      "jp_ephys_viz"
    ]
  }
}
